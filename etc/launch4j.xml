<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
	<headerType>gui</headerType>
	<jar>..\dist\J1939-84.jar</jar>
	<outfile>..\dist\J1939-84.exe</outfile>
	<chdir>.</chdir>
	<priority>high</priority>
	<jre>
		<path>jre</path>
		<initialHeapSize>256</initialHeapSize>
		<maxHeapSize>256</maxHeapSize>
		<opt>-server</opt>
		<opt>-XX:+UseG1GC</opt>
		<opt>-XX:MaxGCPauseMillis=4</opt>
		<opt>-Xlog:gc:file=%TEMP%\j1939-84-gc.txt</opt>
	</jre>
</launch4jConfig>